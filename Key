-- Scripts --

--
GetKeyBtn.MouseButton1Click:Connect(function()
	setclipboard("https://link-target.net/561600/key")
end)

local function validateKey(key)
	return key == "" 
end

EnterBtn.MouseButton1Click:Connect(function()
	local enteredKey = TextBox.Text
	if validateKey(enteredKey) then
		TextBox.PlaceholderText = "Correct Key!"
		TextBox.Text = ""
		wait(1)
		ScreenGui:Destroy()

		-- Loaded --
		loadstring(game:HttpGet("https://raw.githubusercontent.com/snowiwe/BladeBall/main/betaversion",true))()

	else
		TextBox.PlaceholderText = "Invalid key"
		TextBox.Text = ""
		wait(1)
		TextBox.PlaceholderText = "Enter Key"
		TextBox.Text = ""
	end
end)
